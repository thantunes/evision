{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop",
    
      "flex-layout.row#footer-3-desktop"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "flex-layout.col#footer-logotipo",
      "vtex.menu@2.x:menu#Products",
      "vtex.menu@2.x:menu#footer-clothing",
      "vtex.menu@2.x:menu#footer-decoration"
    ],
    "props": {
      "blockClass": "menu-row-footer",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },





  "flex-layout.col#footer-logotipo": {
    "children": ["image#logo-footer","rich-text#logo-footer","flex-layout.col#whatsappflutuante"],
    "props": {
      "blockClass": "logo-footer"
    }
  },


  "flex-layout.col#whatsappflutuante": {
    "children": ["image#whatsappflutuante"],
    "props": {
      "blockClass": "botao-whatsapp-flutuante"
    }
  },



  "image#logo-footer": {
    "props": {
      "src": "/arquivos/logotipo-io.png"
    
    }
  },

  "image#whatsappflutuante": {
    "props": {
      "src": "/arquivos/whats-icon.png",
      "blockClass": "whatsapp-flutuante",
      "link": {
        "url": "https://whts.co/ninjasom",
        "newTab":"true"
      }
    }
  },


  "rich-text#logo-footer": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "NINJA SOM COMERCIO DE ELETRONICOS E MATERIAIS ELETRICOS LTDA - 2012 - 2022, todos os direitos reservados. \n Rua Santa Ifigênia, 556/560/562/564 - Santa Efigênia - CEP: 01.207-000 - São Paulo / SP - CNPJ 07.282.516/0001-15",
      "blockClass": "endereco-footer"
    }
  },



  "social-networks": {
    "props": {
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/vtexonline/"
        },
        {
          "name": "Instagram",
          "url": "https://www.instagram.com/vtextruecloud/"
        },
        {
          "name": "Twitter",
          "url": "https://twitter.com/vtexonline"
        },
        {
          "name": "Youtube",
          "url": "https://www.youtube.com/user/VTEXTV"
        }
      ]
    }
  },

  "flex-layout.row#footer-2-desktop": {
    "children": [
      



  
      "flex-layout.col#blocos-footer-mobile",


      "flex-layout.col#blocos-footer-feito-por-mobile"
    ],
    "props": {
      "blockClass": "pagamento"
    }
  },

  "flex-layout.col#blocos-footer-mobile":{
    "children": [
      "flex-layout.row#blocos-footer-mobile-1",
      "flex-layout.row#blocos-footer-mobile-2"
    ],

    "props": {
      "blockClass": "blocos-footer-mobile"
    }
  },

  "flex-layout.col#blocos-footer-feito-por-mobile":{
"children":[
  "flex-layout.col#feitopor"
],
"props": {
  "blockClass": "blocos-footer-mobile-feito-por"
}

  },


  "flex-layout.row#blocos-footer-mobile-1":{
    "children": [
      "rich-text#tituloinstitucionalfooterpagamentos",
      "flex-layout.row#footer-2-desktop-pagamentos",
      "flex-layout.row#footer-2-desktop-pagamentos-1"
    ]

  },


  
  "flex-layout.row#blocos-footer-mobile-2":{
    "children": [
     
      "rich-text#titulosegurancafooter",
      "flex-layout.row#seguranca-footer"
    ]

  },


  "flex-layout.row#footer-2-desktop-pagamentos":{

    "children": [
   
    ]
  },

  "flex-layout.row#footer-2-desktop-pagamentos-1":{

    "children": [
      "flex-layout.row#bandeiras-1-desktop"
    ]
