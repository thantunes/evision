  "image#logo-amex": {
    "props": {
      "src": "/arquivos/amex-io.png"
    
    }
  },

  "image#logo-pix": {
    "props": {
      "src": "/arquivos/pix-io.png"
    
    }
  },



  "flex-layout.row#bandeiras-2-cont-desktop":{
    "children": [
      "image#logo-boleto",
      "image#logo-diners",
      "image#logo-hipercard",
      "image#logo-elo",
      "image#logo-crediz"

    ],
    "props": {
      "blockClass": "bloco-bandeiras"
    }
  },


  "image#logo-boleto": {
    "props": {
      "src": "/arquivos/boleto-io.png"
    
    }
  },
  "image#logo-diners": {
    "props": {
      "src": "/arquivos/diners-io.png"
    
    }
  },

  "image#logo-hipercard": {
    "props": {
      "src": "/arquivos/elo-io.png"
    
    }
  },

  "image#logo-elo": {
    "props": {
      "src": "/arquivos/hipercard-io.png"
    
    }
  },

  "image#logo-crediz": {
    "props": {
      "src": "/arquivos/crediz-io.png"
    
    }
  },




  "accepted-payment-methods": {
    "props": {
      "paymentMethods": ["MasterCard", "Visa", "Diners Club"]
    }
  },

  "flex-layout.row#footer-3-desktop": {
    "children": [
    "flex-layout.col#feitopor"
  ],
    "props": {
      "blockClass": "credits"
    }
  },

  "flex-layout.col#feitopor":{
    "children": ["rich-text#footer", "image#evision", "image#vtex"
  
  ]
  },



  "rich-text#footer": {
    "props": {
      "text": "Feito com orgulho por",
      "blockClass": "footer"
    }
  },

  "image#evision": {
    "props": {
      "src": "/arquivos/LOGOEVISIONA-io.png",
      "maxHeight":13,
      "blockClass": "logo-evision"
    }
  },

  "image#vtex": {
    "props": {
      "src": "/arquivos/LOGOVTEX-io.png",
      "maxHeight": 24,
      "blockClass": "logo-vtex"
    }
  },

  "footer-layout.mobile": {
    "children": [





      "flex-layout.row#1-footer-mobile",
      "flex-layout.row#2-footer-mobile"
    ]
  },
  "flex-layout.row#2-footer-mobile": {
    "children": ["flex-layout.row#footer-2-desktop"],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4
    }
  },
  "flex-layout.col#footer-1-mobile": {
    "children": [
      "accepted-payment-methods",
      "social-networks",
      "rich-text#footer-mobile"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4,
      "rowGap": 4
    }
  },
  "flex-layout.row#1-footer-mobile": {
    "props": {
      "paddingTop": 4,
      "paddingBottom": 4
    },
    "children": ["flex-layout.col#footer-logotipo","flex-layout.row#redes-sociais-footer","vtex.menu@2.x:menu#footer-mobile"]
  },
  "rich-text#footer-mobile": {
    "props": {
      "text": "All stock and product photos are from photos.icons8.com",
      "blockClass": "footer"
    }
  }
}
